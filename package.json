  "scripts": {
    "fonticons": "gulp font-icons --fontName cs-labs && gulp iconScss2appScss --fontName cs-labs",
    "pug-it": "pug client",
    "pug-it:w": "pug client -P -w",
    "indexFormat": "node indexFormat.js",
    "dev": "concurrently \"tsc -p server -w\" \"npm run pug-it:w\" \"webpack --env=dev --watch --colors --config build/webpack.config.js\"",
    "prod": "npm run pug-it && set NODE_ENV=production&& tsc -p server && webpack --env=prod -p --progress --config build/webpack.config.js && npm run indexFormat",
    "prestart": "tsc -p server",
    "start": "concurrently  \"tsc -p server -w\" \"npm run pug-it:w\" \"webpack --env=dev --watch --colors --config build/webpack.config.js\" \"node dist/server/server.js\""
  },
